	Keywords = {Cosmology: Cosmological Parameters, Cosmology: Theory, Cosmology: Distance Scale, Cosmology: Large-Scale Structure of Universe, Methods: n-Body Simulations},
	Month = nov,
	Pages = {575-588},
	Title = {{Baryonic Acoustic Oscillations in Simulated Galaxy Redshift Surveys}},
