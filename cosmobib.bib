	Journal = {\aap},
	Keywords = {surveys, cosmology: cosmic microwave background, cosmology: cosmological parameters},
	Month = jul,
	Pages = {395-401},
	Title = {{Optimising large galaxy surveys for ISW detection}},
	Volume = {485},
	Year = {2008},
