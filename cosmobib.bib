	Journal = {Astroparticle Physics},
	Month = mar,
	Pages = {591-594},
	Primaryclass = {astro-ph.CO},
	Title = {{Missing power vs low-{$\ell$} alignments in the cosmic microwave background: No correlation in the standard cosmological model}},
	Volume = {34},
