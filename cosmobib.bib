	Archiveprefix = {arXiv},
	Author = {{Larson}, D. and {Dunkley}, J. and {Hinshaw}, G. and {Komatsu}, E. and {Nolta}, M.~R. and {Bennett}, C.~L. and {Gold}, B. and {Halpern}, M. and {Hill}, R.~S. and {Jarosik}, N. and {Kogut}, A. and {Limon}, M. and {Meyer}, S.~S. and {Odegard}, N. and {Page}, L. and {Smith}, K.~M. and {Spergel}, D.~N. and {Tucker}, G.~S. and {Weiland}, J.~L. and {Wollack}, E. and {Wright}, E.~L.},
	Date-Added = {2013-04-11 15:20:40 +0000},
