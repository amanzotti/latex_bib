	Eprint = {1304.3473},
	Journal = {ArXiv e-prints},
	Keywords = {General Relativity and Quantum Cosmology, Astrophysics - Cosmology and Extragalactic Astrophysics, Astrophysics - High Energy Astrophysical Phenomena, High Energy Physics - Phenomenology, High Energy Physics - Theory,Review},
