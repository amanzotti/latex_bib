	Author = {{Van Waerbeke}, L. and {Mellier}, Y. and {Erben}, T. and {Cuillandre}, J.~C. and {Bernardeau}, F. and {Maoli}, R. and {Bertin}, E. and {McCracken}, H.~J. and {Le F{\`e}vre}, O. and {Fort}, B. and {Dantel-Fort}, M. and {Jain}, B. and {Schneider}, P.},
	Date-Added = {2013-04-11 15:20:40 +0000},
	Date-Modified = {2013-04-11 15:20:40 +0000},
	Eprint = {arXiv:astro-ph/0002500},
	Journal = {\aap},
	Keywords = {COSMOLOGY: THEORY, COSMOLOGY: DARK MATTER, COSMOLOGY: GRAVITATIONAL LENSING, COSMOLOGY: LARGE-SCALE STRUCTURE OF UNIVERSE},
