	Journal = {\apj},
	Keywords = {Galaxies: Formation, Gravitation, Instabilities, Cosmology: Large-Scale Structure of Universe},
	Month = feb,
	Pages = {47-59},
	Title = {{Nonlinear Gravitational Growth of Large-Scale Structures Inside and Outside Standard Cosmology}},
