	Author = {{Nicola}, A. and {Refregier}, A. and {Amara}, A.},
	Date-Added = {2016-12-28 16:11:53 +0000},
	Date-Modified = {2016-12-28 16:11:54 +0000},
	Eprint = {1612.03121},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = dec,
	Title = {{Integrated Cosmological Probes: Extended Analysis}},
	Year = 2016,
	Bdsk-File-1 = {},
