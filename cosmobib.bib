	Journal = {\prd},
	Keywords = {Cosmology, Dark matter, Dark energy, Galaxy groups, clusters, and superclusters; large scale structure of the Universe},
	Month = oct,
	Number = {8},
	Pages = {083519},
	Title = {{Nonlinear perturbation theory with halo bias and redshift-space distortions via the Lagrangian picture}},
	Volume = {78},
	Year = {2008},
