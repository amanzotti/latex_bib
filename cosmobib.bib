	Keywords = {methods: statistical, galaxies: clusters: general, cosmic background radiation, cosmology: theory},
	Month = dec,
	Pages = {2207-2218},
	Primaryclass = {astro-ph.CO},
	Title = {{The ISW-tSZ cross-correlation: integrated Sachs-Wolfe extraction out of pure cosmic microwave background data}},
	Volume = 418,
	Year = 2011,
	Bdsk-File-1 = {},
