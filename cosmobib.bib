	Eprint = {1011.4477},
	Journal = {\jcap},
	Month = jun,
	Pages = {15},
	Primaryclass = {astro-ph.CO},
	Title = {{Next-to-leading resummations in cosmological perturbation theory}},
	Volume = {6},
	Year = {2011},
